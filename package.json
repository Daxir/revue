{
  "name": "remix-template-remix",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev": "remix dev",
    "dev:test": "cross-env NODE_ENV=test dotenv -e .env.test remix dev",
    "start": "remix-serve build",
    "lint": "eslint app --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint app --ext .js,.jsx,.ts,.tsx --fix",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "test": "npm run docker:up && dotenv -e .env.test npx prisma migrate deploy && dotenv -e .env.test vitest",
    "test:run": "npm run docker:up && dotenv -e .env.test npx prisma migrate deploy && dotenv -e .env.test vitest run",
    "test:ui": "npm run docker:up && dotenv -e .env.test npx prisma migrate deploy && dotenv -e .env.test -- vitest --ui",
    "playwright:test": "npm run docker:up && dotenv -e .env.test npx prisma migrate deploy && dotenv -e .env.test start-server-and-test dev:test http://localhost:3000 'dotenv -e .env.test playwright test'",
    "posttest": "npm run docker:down",
    "posttest:run": "npm run docker:down",
    "posttest:ui": "npm run docker:down",
    "postplaywright:test": "npm run docker:down"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/server": "^11.10.0",
    "@mantine/carousel": "^5.7.1",
    "@mantine/core": "^5.7.1",
    "@mantine/dropzone": "^5.7.1",
    "@mantine/form": "^5.7.1",
    "@mantine/hooks": "^5.7.1",
    "@mantine/modals": "^5.7.1",
    "@mantine/notifications": "^5.7.1",
    "@mantine/remix": "^5.7.1",
    "@prisma/client": "^4.5.0",
    "@remix-run/node": "^1.7.3",
    "@remix-run/react": "^1.7.3",
    "@remix-run/serve": "^1.7.3",
    "@remix-run/vercel": "^1.7.3",
    "@remix-validated-form/with-yup": "^2.0.1",
    "@supabase/supabase-js": "^2.0.6",
    "@tabler/icons": "^1.107.0",
    "bcrypt": "^5.1.0",
    "csv-parse": "^5.3.3",
    "embla-carousel-react": "^7.0.3",
    "react": "^18.2.0",
    "react-circle-flags": "^0.0.18",
    "react-countup": "^6.3.2",
    "react-dom": "^18.2.0",
    "react-papaparse": "^4.1.0",
    "remix-auth": "^3.3.0",
    "remix-auth-form": "^1.2.0",
    "remix-auth-socials": "^1.0.2",
    "remix-utils": "^3.4.0",
    "remix-validated-form": "^4.5.7",
    "spin-delay": "^1.2.0",
    "tiny-invariant": "^1.3.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@playwright/test": "^1.28.1",
    "@remix-run/dev": "^1.7.3",
    "@remix-run/eslint-config": "^1.7.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@trivago/prettier-plugin-sort-imports": "^3.4.0",
    "@types/bcrypt": "^5.0.0",
    "@types/react": "^18.0.23",
    "@types/react-dom": "^18.0.7",
    "@types/papaparse": "^5.3.5",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "@vitejs/plugin-react": "^2.1.0",
    "@vitest/ui": "^0.24.3",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-playwright": "^0.11.2",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.9.1",
    "eslint-plugin-unicorn": "^44.0.2",
    "happy-dom": "^7.6.6",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "prisma": "^4.5.0",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.8.4",
    "vite-tsconfig-paths": "^3.5.2",
    "vitest": "^0.24.3"
  },
  "engines": {
    "node": ">=14"
  }
}
